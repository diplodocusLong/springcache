<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lianglong.cache.mapper.EmployeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lianglong.cache.entity.Employee">
        <id column="id" property="id" />
        <result column="emp_name" property="empName" />
        <result column="emp_age" property="empAge" />
        <result column="dept_id" property="deptId" />
        <result column="create_date" property="createDate" />
        <result column="update_time" property="updateTime" />
        <result column="is_out" property="isOut" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, emp_name, emp_age, dept_id, create_date, update_time, is_out, version
    </sql>


</mapper>
